  },
  "dependencies": {
    "commander": "^2.20.3",
    "path": "^0.12.7",
    "path-parse": "1.0.7"
  },
  "devDependencies": {
    "@types/node": "^6.14.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
